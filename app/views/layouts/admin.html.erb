<!DOCTYPE html>
<html>
  <head>
    <title><%= yield :title %></title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag "application", media: "all" %>
    <%= stylesheet_link_tag "http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" %>
    <%= stylesheet_link_tag "http://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons" %>

    <%= javascript_include_tag "application" %>

    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
  </head>
  <body>
    <div class="wrapper">
      <%= render "layouts/header_admin" %>
      <%= yield %>
      <%= render "layouts/footer_admin" %>
    </div>
    <section class="modal-content"></section>
  </body>
</html>
